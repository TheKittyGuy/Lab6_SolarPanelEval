   ],
   "source": [
    "columns = ['voltage_art1', 'voltage_room1', 'voltage_art2', 'voltage_room2',\n",
    "           'current_art1', 'current_room1', 'current_art2', 'current_room2']\n",
    "\n",
    "# Dictionary to store mean and standard deviation for each column\n",
    "statistics = {}\n",
